package exercicios;

import java.util.Scanner;

public class Ex4 {

	public static void main(String[] args) {

		
		Scanner sc = new Scanner(System.in);
		
		int A = sc.nextInt();
		int B = sc.nextInt();
		int C = sc.nextInt();
		int D = sc.nextInt();
		
		int diferenca = (A * B) - (C * D);
		
		
		System.out.printf("A diferença do produto de %d * %d e %d * %d é de : %d", A, B, C, D, diferenca);
		
	}

}
